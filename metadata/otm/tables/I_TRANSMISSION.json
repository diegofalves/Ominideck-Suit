      "size": 1,
      "isNull": false,
      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": "'Y'"
    },
    {
      "name": "SENDER_TRANSMISSION_NO",
      "description": "The SENDER_TRANSMISSION_NO column specifies the transmission number that the sender assigned to the transmission.",
      "dataType": "NUMBER",
      "isNull": true,
      "isConstraint": false,
