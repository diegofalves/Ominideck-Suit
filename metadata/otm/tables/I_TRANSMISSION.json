{
  "table": {
    "schema": "glogowner",
    "name": "i_transmission",
    "description": "The I_TRANSMISSION table stores all XML transmissions which are received by GLog, or sent out of Glog. This table is an audit trail for the XML transmissions sent to the GC3 system. These transmissions can be query transmissions or may contain one or more transactions to be carried out by GC3. The actual transmission is stored as a LONG variable. The details abut the user sending the transmission are also stored."
  },
  "columns": [
    {
      "name": "I_TRANSMISSION_NO",
      "description": "The I_TRANSMISSION_NO column uniquely identifies the transmission.",
      "dataType": "NUMBER",
      "isNull": false,
      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": ""
    },
    {
      "name": "TRANSMISSION_TYPE",
      "description": "",
      "dataType": "VARCHAR2",
      "size": 30,
      "isNull": true,
      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": ""
    },
    {
      "name": "CREATE_DATE",
      "description": "The CREATE_DATE column specifies when the transmission was first created.",
      "dataType": "DATE",
      "isNull": false,
      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": "SYSDATE"
    },
    {
      "name": "TRANSACTION_COUNT",
      "description": "The TRANSACTION_COUNT column specifies the number of transactions associated with this transmission.",
