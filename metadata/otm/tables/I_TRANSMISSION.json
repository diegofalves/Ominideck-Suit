      "isNull": true,
      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": ""
    },
    {
      "name": "IS_INBOUND",
      "description": "The IS_INBOUND column indicates that a transmission is either inbound (value equals Y) or not (value equals N).",
