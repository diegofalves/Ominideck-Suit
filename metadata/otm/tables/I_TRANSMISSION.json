      "size": 2000,
      "isNull": true,
      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": ""
    },
    {
      "name": "USERNAME",
      "description": "The USERNAME column specifies the GLog username used to log into GLog to persist the transmission. This is required when IS_INBOUND is Y.",
      "dataType": "VARCHAR2",
      "size": 128,
      "isNull": true,
      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": ""
    },
    {
      "name": "PASSWORD",
      "description": "The PASSWORD column specifies the GLog password used to log into GLog to persist the transmission. This is required when IS_INBOUND is Y.",
