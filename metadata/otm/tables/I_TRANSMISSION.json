      "isConstraint": false,
      "constraintValues": "",
      "conditionalConstraint": "",
      "defaultValue": ""
    },
    {
      "name": "RECEIVER_HOST_NAME",
      "description": "The SENDER_HOST_NAME column specifies the name of the machine that received the transmission.",
